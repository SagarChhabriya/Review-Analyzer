{
  "version": 2,
  "builds": [
    { "src": "Backend/**/*.py", "use": "@vercel/python" },
    { "src": "Frontend/**/*.{js,jsx,css}", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/analyze", "dest": "/Backend/app.py" },
    { "src": "/(.*)", "dest": "/Frontend/index.html" }
  ]
}
